# Clerk Authentication

# These variables are used for Clerk user authentication and management.

NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_publishable_key # Your Clerk publishable key for the frontend.

CLERK_SECRET_KEY=your_secret_key # Your Clerk secret key for the backend.

NEXT_PUBLIC_CLERK_SIGN_IN_URL=your_sign_in_url # The URL for your Clerk sign-in page.

NEXT_PUBLIC_CLERK_SIGN_UP_URL=your_sign_up_url # The URL for your Clerk sign-up page.

CLERK_SIGN_IN_FORCE_REDIRECT_URL=your_sign_in_redirect_url # Optional redirect URL after sign-in.

CLERK_SIGN_UP_FORCE_REDIRECT_URL=your_sign_up_redirect_url # Optional redirect URL after sign-up. # Database Configuration

# These variables are used for database connection and interaction.

DATABASE_URL=your_database_url # The connection string for your database. # Supabase Configuration

# These variables are used for Supabase services.

SUPABASE_KEY=your_supabase_key # Your Supabase API key. # Pinecone Configuration

# These variables are used for connecting to your Pinecone vector database.

PINECONE_ENVIRONMENT=your_pinecone_environment # The environment where your Pinecone index is hosted.

PINECONE_API_KEY=your_pinecone_api_key # Your Pinecone API key. # AI Model API Keys

# These variables are used for accessing AI APIs.

GEMINI_API_KEY=your_gemini_api_key # Your API key for Google Gemini.

# These variables are used to connect to an Upstash Redis database.

UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url # The REST URL for your Upstash Redis instance.

UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token # The REST token for your Upstash Redis instance. # Liveblocks Configuration

# This is used for the Liveblocks real-time collaboration platform.

LIVEBLOCKS_SECRET_KEY=your_liveblocks_secret_key # Your Liveblocks secret key.